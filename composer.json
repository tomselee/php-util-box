
{
    "name": "tomselee/php-util-box",
    "description": "封装常用工具类或者方法",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "tomse",
            "email": "tomselee@qq.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.0",
        "monolog/monolog": "^1.12"
    },
    "require-dev": {
        "php": ">=7.0",
        "friendsofphp/php-cs-fixer": "*",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "UtilBox\\": "src/",
            "Tests\\": "tests/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "scripts": {
        "fix": "@php ./vendor/friendsofphp/php-cs-fixer/php-cs-fixer fix --config=./src/PHPCS/php-cs-fixer.php"
    },
    "version": "1.0.0"
}
